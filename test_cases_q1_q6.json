{
  "test_cases": [
    {
      "id": "Q1",
      "question": "SSの新SS(W)が納期ギリギリのため納期20分前に処理完了となるよう配置したいです。最適配置を教えてください。",
      "expected_elements": [
        "納期",
        "20分前",
        "必要人数",
        "配置",
        "業務区分の4階層明示"
      ],
      "should_include": [
        "SS",
        "新SS(W)",
        "OCR対象",
        "エントリ"
      ],
      "should_not_include": [
        "拠点から拠点への移動のみ"
      ]
    },
    {
      "id": "Q2",
      "question": "配置転換元の工程は大丈夫ですか?移動元の処理に影響はありますか?",
      "expected_elements": [
        "移動元の状況",
        "影響分析",
        "人員数の確認"
      ],
      "should_include": [
        "移動元",
        "影響"
      ]
    },
    {
      "id": "Q3",
      "question": "SSの16:40受信分を優先的に処理したいです。非SSから何人移動させたらよいですか?",
      "expected_elements": [
        "16:40受信分",
        "非SSから",
        "移動人数",
        "業務区分明示"
      ],
      "should_include": [
        "非SS",
        "SS",
        "人"
      ]
    },
    {
      "id": "Q4",
      "question": "SS15:40受信分と適徴15:40受信分の処理は現在の配置だと何時に終了する想定ですか",
      "expected_elements": [
        "完了時刻の予測",
        "SS 15:40",
        "適徴 15:40",
        "現在の配置"
      ],
      "should_include": [
        "時",
        "分",
        "完了"
      ]
    },
    {
      "id": "Q5",
      "question": "あはきを16:40頃までに処理完了させるためには各工程何人ずつ配置したら良いですか",
      "expected_elements": [
        "あはき",
        "16:40",
        "各工程",
        "人数配分"
      ],
      "should_include": [
        "エントリ",
        "補正",
        "人"
      ]
    },
    {
      "id": "Q6",
      "question": "現在の配置でそれぞれの納期までに遅延が発生する見込みがある工程はありますか",
      "expected_elements": [
        "遅延リスク",
        "工程リスト",
        "納期",
        "現在の配置"
      ],
      "should_include": [
        "遅延",
        "工程"
      ]
    }
  ]
}
